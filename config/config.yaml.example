# GOTRS Local Configuration Override
# Copy this file to config.yaml for local development overrides
# These values override default.yaml
# DO NOT commit config.yaml to version control

# Example overrides for local development:

app:
  debug: true
  env: development

database:
  host: localhost
  password: my_secure_password

redis:
  host: localhost

auth:
  jwt:
    secret: "my-local-development-secret-key-change-in-production"

email:
  smtp:
    host: localhost
    port: 1025